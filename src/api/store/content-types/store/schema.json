{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "store"
    },
    "code": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::code.code",
      "inversedBy": "stores"
    },
    "doc": {
      "type": "string"
    },
    "platform": {
      "type": "enumeration",
      "enum": [
        "Loja Integrada",
        "E-com.plus",
        "Tray",
        "Bagy/Dooca",
        "NuvemShop"
      ]
    }
  }
}
